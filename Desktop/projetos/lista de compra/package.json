{
  "name": "lista-de-compras",
  "version": "1.0.0",
  "description": "Aplicação de Lista de Compras com React e Express + MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "deploy": "cd client && npm run deploy",
    "predeploy": "cd client && npm run build"
  },
  "keywords": [
    "lista",
    "compras",
    "react",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
